{"ast":null,"code":"import TaskAddingFeature from '@/components/TaskComponents/TaskAddingFeature.vue';\nimport TaskUpdatingFeature from '@/components/TaskComponents/TaskUpdatingFeature.vue';\nexport default {\n  components: {\n    TaskAddingFeature,\n    TaskUpdatingFeature\n  },\n  data() {\n    return {\n      taskItems: JSON.parse(localStorage.getItem('taskItems')) || []\n    };\n  },\n  watch: {\n    taskItems() {\n      localStorage.setItem('taskItems', JSON.stringify(this.taskItems));\n    }\n  },\n  computed: {\n    tasksExisted() {\n      if ((JSON.parse(localStorage.getItem('taskItems')) || []).length == 0) return false;else return true;\n    },\n    computed: {\n      taskItems() {\n        return JSON.parse(localStorage.getItem('taskItems')) || [];\n      }\n    }\n  },\n  methods: {\n    reloadPage() {\n      window.location.reload();\n    },\n    updateTask($event) {\n      if ({\n        ...$event\n      }.id - 1 < this.taskItems.length && {\n        ...$event\n      }.id - 1 >= 0) {\n        if ({\n          ...$event\n        }.name) this.taskItems[{\n          ...$event\n        }.description] = {\n          ...$event\n        }.description;\n        this.taskItems[{\n          ...$event\n        }.duration] = {\n          ...$event\n        }.duration;\n        this.taskItems[{\n          ...$event\n        }.difficulty] = {\n          ...$event\n        }.difficulty;\n        this.taskItems[{\n          ...$event\n        }.start] = {\n          ...$event\n        }.description;\n        this.taskItems[{\n          ...$event\n        }.id - 1] = {\n          ...$event\n        };\n        localStorage.setItem('taskItems', JSON.stringify(this.taskItems));\n        this.reloadPage();\n      } else {\n        alert(\"you entered a bad id\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["TaskAddingFeature","TaskUpdatingFeature","components","data","taskItems","JSON","parse","localStorage","getItem","watch","setItem","stringify","computed","tasksExisted","length","methods","reloadPage","window","location","reload","updateTask","$event","id","name","description","duration","difficulty","start","alert"],"sources":["src/views/Task/TaskPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- Adding the adding task feature component to the page  -->\r\n    <!-- إضافة المكون  *  ميزة إضافة مهمة  * إلى الصفحة  -->\r\n    <task-adding-feature\r\n    \r\n      @addingTask=\"\r\n        {\r\n          taskItems.push({ id: taskItems.length + 1, ...$event }), reloadPage();\r\n        }\r\n      \"\r\n    >\r\n    </task-adding-feature>\r\n    <br />\r\n    <!-- taskUpdatingFeature -->\r\n    <task-updating-feature v-if=\"tasksExisted\" @updatingTask=\"updateTask\"> </task-updating-feature>\r\n\r\n    <!-- Adding the task list component to the page  -->\r\n    <!-- إضافة المكون  *  جدول المهام  * إلى الصفحة  -->\r\n    <data-table :itemsList=\"taskItems\"></data-table>\r\n  </div>\r\n</template>\r\n<script lang=\"js\">\r\nimport TaskAddingFeature from '@/components/TaskComponents/TaskAddingFeature.vue';\r\nimport TaskUpdatingFeature from '@/components/TaskComponents/TaskUpdatingFeature.vue';\r\n\r\nexport default {\r\n    components:{\r\n      TaskAddingFeature,\r\n      TaskUpdatingFeature\r\n    },\r\n    data(){\r\n      return {\r\n        taskItems:JSON.parse(localStorage.getItem('taskItems'))||[]\r\n    }},\r\n        watch:{\r\n         taskItems(){\r\n            localStorage.setItem('taskItems',JSON.stringify(this.taskItems))\r\n         }\r\n    }\r\n    ,\r\n    computed:{\r\n      tasksExisted()\r\n      {\r\n        if((JSON.parse(localStorage.getItem('taskItems'))||[]).length==0) return false;\r\n      else return true;\r\n    },\r\n    computed:{\r\n      taskItems(){\r\n        return JSON.parse(localStorage.getItem('taskItems'))||[]\r\n      }\r\n    }\r\n  },\r\n    methods:{\r\n        reloadPage()\r\n        {\r\n            window.location.reload();\r\n        },\r\n        updateTask($event){\r\n          if((({...$event}.id-1)<this.taskItems.length)&&(({...$event}.id-1)>=0 ))\r\n          {\r\n            if ({...$event}.name)\r\n             this.taskItems[{...$event}.description]={...$event}.description;\r\n             this.taskItems[{...$event}.duration]={...$event}.duration;\r\n             this.taskItems[{...$event}.difficulty]={...$event}.difficulty;\r\n             this.taskItems[{...$event}.start]={...$event}.description;\r\n\r\n             this.taskItems[{...$event}.id-1]={ ...$event };\r\n             localStorage.setItem('taskItems',JSON.stringify(this.taskItems));\r\n             this.reloadPage()\r\n          }\r\n          else\r\n          {\r\n            alert(\"you entered a bad id\")\r\n          }\r\n      }\r\n    }\r\n}\r\n</script>\r\n"],"mappings":"AAuBA,OAAAA,iBAAA;AACA,OAAAC,mBAAA;AAEA;EACAC,UAAA;IACAF,iBAAA;IACAC;EACA;EACAE,KAAA;IACA;MACAC,SAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EAAA;EACAC,KAAA;IACAL,UAAA;MACAG,YAAA,CAAAG,OAAA,cAAAL,IAAA,CAAAM,SAAA,MAAAP,SAAA;IACA;EACA;EAEAQ,QAAA;IACAC,aAAA,EACA;MACA,KAAAR,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA,sBAAAM,MAAA,yBACA;IACA;IACAF,QAAA;MACAR,UAAA;QACA,OAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACA;IACA;EACA;EACAO,OAAA;IACAC,WAAA,EACA;MACAC,MAAA,CAAAC,QAAA,CAAAC,MAAA;IACA;IACAC,WAAAC,MAAA;MACA;QAAA,GAAAA;MAAA,EAAAC,EAAA,YAAAlB,SAAA,CAAAU,MAAA;QAAA,GAAAO;MAAA,EAAAC,EAAA,WACA;QACA;UAAA,GAAAD;QAAA,EAAAE,IAAA,EACA,KAAAnB,SAAA;UAAA,GAAAiB;QAAA,EAAAG,WAAA;UAAA,GAAAH;QAAA,EAAAG,WAAA;QACA,KAAApB,SAAA;UAAA,GAAAiB;QAAA,EAAAI,QAAA;UAAA,GAAAJ;QAAA,EAAAI,QAAA;QACA,KAAArB,SAAA;UAAA,GAAAiB;QAAA,EAAAK,UAAA;UAAA,GAAAL;QAAA,EAAAK,UAAA;QACA,KAAAtB,SAAA;UAAA,GAAAiB;QAAA,EAAAM,KAAA;UAAA,GAAAN;QAAA,EAAAG,WAAA;QAEA,KAAApB,SAAA;UAAA,GAAAiB;QAAA,EAAAC,EAAA;UAAA,GAAAD;QAAA;QACAd,YAAA,CAAAG,OAAA,cAAAL,IAAA,CAAAM,SAAA,MAAAP,SAAA;QACA,KAAAY,UAAA;MACA,OAEA;QACAY,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}